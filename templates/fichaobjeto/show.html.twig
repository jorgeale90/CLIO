{% extends 'base.html.twig' %}
{% block titlepage %}Ficha Objeto Patrimonial{% endblock %}
{% block head %}
    <!-- Include SmartTab CSS -->
    <link href="{{ asset('page/tab/smart_tab_all.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- owl carousel css -->
    <link rel="stylesheet" type="text/css" href="{{ asset('page/bower_components/owl.carousel/css/owl.carousel.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('page/bower_components/owl.carousel/css/owl.theme.default.css') }}">
    <!-- swiper css -->
    <link rel="stylesheet" type="text/css" href="{{ asset('page/bower_components/swiper/css/swiper.min.css') }}">
    <!-- light-box css -->
    <link rel="stylesheet" type="text/css" href="{{ asset('page/bower_components/ekko-lightbox/css/ekko-lightbox.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('page/bower_components/lightbox2/css/lightbox.css') }}">
{% endblock %}
{% block page_header %}
    <div class="card">
        <div class="card-block front-icon-breadcrumb row">
            <div class="breadcrumb-header col">
                <div class="d-inline-block">
                    <h5>Objeto</h5>
                </div>
            </div>
            <div class="col-lg-8">
                <div class="page-header-breadcrumb">
                    <ul class="breadcrumb-title">
                        <li class="breadcrumb-item"><a href="#!">Objeto</a>
                        </li>
                        <li class="breadcrumb-item"><a href="#!">Listado Ficha Objeto Patrimonial</a>
                        </li>
                        <li class="breadcrumb-item active"><a href="#!">Mostrar Ficha Objeto Patrimonial</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block contenido %}
