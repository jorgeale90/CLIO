        <div class="card gallery-desc">
            <div class="masonry-media">
                <a class="media-middle" href="#!">
                    <div class="owl-carousel carousel-dot owl-theme">
                        {% for por in fich.portadaobjeto %}
                            <img src="{{ asset('/CLIO/public/uploads/objeto/portada/' ~ por.link) }}" alt="{{ fich.nombreobjeto }}" width="200px" height="280px">
                        {% endfor %}
                    </div>
                </a>
            </div>
            <div class="card-block">
                <h6 class="job-card-desc"><center>{{ fich.nombreobjeto }}</center></h6>
                <h7 class="job-meta-data"><center>{{ fich.clasificacion }}</center></h7>
                <p><h7 class="job-meta-data"><center style="color: #00AEEF">Código de la Ficha del Objeto</center></h7>
                <h7 class="job-meta-data"><center style="color: #00AEEF">{{ fich.codigoobjeto }}</center></h7></p>
                <div class="job-meta-data"><i class="icofont icofont-pin"></i>País: {{ fich.pais }}</div>
                <div class="job-meta-data"><i class="icofont icofont-pin"></i>Lugar Procedencia: {{ fich.lugarprocedencia }}</div>
                <div class="col-sm-13">
                    <a class="btn " style="background-color:transparent" href="{{ path('fichaobjeto_edit', {'id': fich.id}) }}"><span style="color:#010d0c"><i class="icofont icofont-edit"></i></a>
                    <a class="btn " style="background-color:transparent" href="{{ path('fichaobjeto_index') }}"><span style="color:#010d0c" ><i class="icofont icofont-list"></i></a>
                    <a class="btn " style="background-color:transparent" href="#"><span style="color:#010d0c"><i class="icofont icofont-delete-alt"></i></a>
                </div>
            </div>
        </div>
    </div>

