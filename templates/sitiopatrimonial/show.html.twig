        <div class="card gallery-desc">
            <div class="masonry-media">
                <a class="media-middle" href="#!">
                    <div class="owl-carousel carousel-dot owl-theme">
                        {% for foto in sitio.fotografias %}
                            <img src="{{ asset('/CLIO/public/uploads/sitio/fotografias/' ~ foto.link) }}" alt="masonary" width="200px" height="280px">
                        {% endfor %}
                    </div>
                </a>
            </div>
            <div class="card-block">
                <h6 class="job-card-desc"><center>{{ sitio.nombre }}</center></h6>
                <h7 class="job-meta-data"><center>{{ sitio.categoria }}</center></h7>
                <p><h7 class="job-meta-data"><center style="color: #00AEEF">Código del Sitio Patrimonial</center></h7>
                <h7 class="job-meta-data"><center style="color: #00AEEF">{{ sitio.codigo }}</center></h7></p>
                <div class="job-meta-data"><i class="icofont icofont-pin"></i>Provincia: {{ sitio.provincia }}</div>
                <div class="job-meta-data"><i class="icofont icofont-pin"></i>Municipio: {{ sitio.municipio }}</div>
                <div class="col-sm-13">
                    <a class="btn " style="background-color:transparent" href="{{ path('sitiopatrimonial_edit', {'id': sitio.id}) }}"><span style="color:#010d0c"><i class="icofont icofont-edit"></i></a>
                    <a class="btn " style="background-color:transparent" href="{{ path('sitiopatrimonial_index') }}"><span style="color:#010d0c"><i class="icofont icofont-list"></i></a>
                    <a class="btn " style="background-color:transparent" href="#"><span style="color:#010d0c"><i class="icofont icofont-delete-alt"></i></a>
                </div>
            </div>
        </div>
    </div>

