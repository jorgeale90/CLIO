                </div>

                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">{{ form_label(form.tipointervencion, 'Tipo de Intervención :') }}</label>
                    <div class="col-sm-10">
                        {{ form_widget(form.tipointervencion, { 'attr': { 'disabled':'disabled' } }) }}
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">{{ form_label(form.especialistajefe, 'Especialista Jefe :') }}</label>
                    <div class="col-sm-10">
                        {{ form_widget(form.especialistajefe, { 'attr': { 'disabled':'disabled' } }) }}
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-sm-2 col-form-label">{{ form_label(form.especialistapart, 'Participantes :') }}</label>
                    <div class="col-sm-10">
                        {{ form_widget(form.especialistapart, { 'attr': {'disabled':'disabled' } }) }}
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-md-12">
                        <a href="{{ path('especialist') }}" style="background-color:transparent" class="btn btn-mat btn-info f-right"><span style="color:#010d0c"><i class="icofont icofont-exchange"></i> Volver al Listado</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}